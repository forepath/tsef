{
  "$schema": "https://json-schema.org/schema",
  "$id": "Native",
  "title": "Device Native (iOS, Android) Ionic Application Generator",
  "description": "Nx generator for creating a new Ionic and Angular device native (iOS, Android) application",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "prefix": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "default": "app",
      "x-prompt": "What prefix would you like to use?"
    },
    "protected": {
      "type": "boolean",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 2
      },
      "default": true,
      "x-prompt": "Enable authenticated routes?"
    },
    "capacitor": {
      "type": "boolean",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 3
      },
      "default": true,
      "x-prompt": "Enable capacitor?"
    }
  },
  "required": ["name"]
}
